<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{mm:ss.SSS} - %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="org.web3j.protocol" level="INFO"/>

    <root level="TRACE">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>